{
  "manifest_version": 3,
  "name": "Tab Manager",
  "version": "1.0",
  "description": "Save open tabs as a group in Chrome",
  "action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}


